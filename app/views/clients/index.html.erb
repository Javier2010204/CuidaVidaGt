<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Nombre</th>
      <th scope="col">Telefono</th>
      <th scope="col">Edad</th>
    </tr>
  </thead>
  <tbody>
    <% @clients.each do |client|%>
        <tr>
          <th scope="row"><%= client.id %></th>
          <td><%= client.name %></td>
          <td><%= client.phone %></td>
          <td><%= client.age %></td>
          <td>
            <%= link_to 'Ver mas...', client%>
          </td>
        </tr>
    <% end %>
  </tbody>
</table>
